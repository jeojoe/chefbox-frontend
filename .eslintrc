{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "plugins": [
    "react",
    "jsx-a11y",
    "import"
  ],
  "rules": {
    "react/jsx-filename-extension": 0,
    "no-use-before-define": 0,
    // "react/prop-types": 0,
    "import/prefer-default-export": 0,
    "react/jsx-max-props-per-line": 0,
    "react/no-did-mount-set-state": 0,
    "no-underscore-dangle": 0,
    "no-plusplus": 0
  }
}